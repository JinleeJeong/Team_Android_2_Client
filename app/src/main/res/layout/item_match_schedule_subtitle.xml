<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>
        <variable
            name="matchList"
            type="kr.yapp.teamplay.domain.entity.MatchList" />
    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingHorizontal="20dp"
        android:paddingVertical="15dp">

        <TextView
            android:id="@+id/match_schedule_subtitle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="123"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <View
            android:layout_width="0dp"
            android:layout_height="1sp"
            android:layout_marginLeft="14dp"
            android:background="@color/colorVeryLightPink"
            app:layout_constraintBottom_toBottomOf="@+id/match_schedule_subtitle"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/match_schedule_subtitle"
            app:layout_constraintTop_toTopOf="@+id/match_schedule_subtitle" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>